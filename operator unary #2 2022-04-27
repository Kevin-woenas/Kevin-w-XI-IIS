  #include <iostream>
  #include <math.h>
  using namespace std;
  
  int main (){
      float a = sqrt (15);//menghitung akar kuadrat
      float b = cbrt (27);//akar kubik
      float c = pow(4.334,2.231);//menghitung pangkat
      float d = sin(512);//sin
      float e = round (3.12);
      //tambahkan dan tampilkan d=sin e=cos f=tan
      
      
      cout<<a<<endl;
      cout<<b<<endl;
      cout<<c<<endl;
      cout<<d<<endl;
      cout<<e<<endl;
     
      return 0;
